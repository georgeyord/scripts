dock:
  image: crosbymichael/skydock:latest
  command: -ttl 30 -environment local -s /docker.sock -domain mydockers.com
  volumes:
   - /var/run/docker.sock:/docker.sock
  links:
   - dns:skydns
dns:
  image: crosbymichael/skydns:latest
  command: -nameserver 8.8.8.8:53 -domain docker.local
  ports:
   - 8008:8080
   - 172.17.42.1:53:53/udp
  volumes:
   - /var/run/docker.sock:/docker.sock
